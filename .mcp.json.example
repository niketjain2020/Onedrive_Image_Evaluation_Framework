{
  "mcpServers": {
    "gemini": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "mcp-server-gemini"],
      "env": {
        "GEMINI_API_KEY": "${GEMINI_API_KEY}",
        "GEMINI_MODEL": "gemini-2.0-flash"
      }
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@playwright/mcp"]
    }
  }
}
